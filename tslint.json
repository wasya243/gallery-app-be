{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "ter-arrow-parens": [false, "always"],
    "arrow-parens": [false, "ban-single-arg-parens"],
    "no-console": false,
    "quotemark": [
      true,
      "single"
    ],
    "max-line-length": [
      true,
      {
        "limit": 200
      }
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        }
      }
    ]
  },
  "rulesDirectory": []
}